<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record id="view_picking_shipping_fedex" model="ir.ui.view">
			<field name="name">stock.picking.shipping.fedex</field>
			<field name="model">stock.picking</field>
			<field name="inherit_id" ref="stock.view_picking_out_form" />
			<field name="type">form</field>
			<field name="priority" eval="20" />
			<field name="arch" type="xml">
				<xpath expr="//separator[@string='Shipment']" position="before">
					<group colspan="4" col="4" attrs="{'invisible':[('ship_company_code','!=','fedex')]}" >
						<separator string="Fedex" colspan="4" />
						<field name='fedex_dropoff_type' attrs="{'required': [('ship_company_code','=','fedex')]}"  />
						<field name='fedex_service_type' attrs="{'required': [('ship_company_code','=','fedex')]}"  />
						<field name='fedex_packaging_type'  attrs="{'required': [('ship_company_code','=','fedex')]}" />
						<field name='fedex_package_detail'  attrs="{'required': [('ship_company_code','=','fedex')]}"  />
						<field name='fedex_payment_type'  attrs="{'required': [('ship_company_code','=','fedex')]}"  />
						<field name='fedex_physical_packaging'   attrs="{'required': [('ship_company_code','=','fedex')]}" />
					</group>
				</xpath>
			</field>
		</record>
	</data>
</openerp>
    